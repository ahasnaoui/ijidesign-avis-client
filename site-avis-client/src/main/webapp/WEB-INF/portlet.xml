	<portlet>
		<description>Portlet Site Avis Client</description>
		<portlet-name>ite-site-avis-client-portlet</portlet-name>
		<display-name>Site Avis Client Portlet</display-name>

		<portlet-class>org.springframework.web.portlet.DispatcherPortlet</portlet-class>
		<init-param>
			<name>contextConfigLocation</name>
			<value>/WEB-INF/context/siteAvisClientPortlet.xml</value>
		</init-param>
		<init-param>
			<name>view-template</name>
			<value>/html/view.jsp</value>
		</init-param>
		
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		
		<supported-locale>fr</supported-locale>

		<portlet-info>
			<title>Site Avis Client Portlet</title>
			<short-title>Site Avis Client</short-title>
			<keywords>site,avis,client</keywords>
		</portlet-info>
	</portlet>
	
