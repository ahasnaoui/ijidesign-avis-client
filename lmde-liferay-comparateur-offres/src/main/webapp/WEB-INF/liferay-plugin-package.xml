<?xml version="1.0"?>
<!DOCTYPE plugin-package PUBLIC "-//Liferay//DTD Plugin Package 6.2.0//EN" "http://www.liferay.com/dtd/liferay-plugin-package_6_2_0.dtd">

<plugin-package>
    <name>${project.name}</name>
    <module-id>${project.groupId}/${parent.artifactId}/${project.version}/${project.packaging}</module-id>
    <recommended-deployment-context>${project.artifactId}-portlet</recommended-deployment-context>
    <types>
        <type>portlet</type>
    </types>
    <tags>
        <tag>Devis en ligne</tag>
    </tags>
    <short-description>${project.description}</short-description>
    <long-description></long-description>
    <change-log></change-log>
    <author>Orange Business Services - Orange Application for Businness</author>
    <licenses>
        <license osi-approved="false">proprietary</license>
    </licenses>
    <liferay-versions>
        <liferay-version>${liferay.version}+</liferay-version>
    </liferay-versions>
</plugin-package>